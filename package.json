{
  "name": "integrate-prettier-with-eslint",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write app.js",
    "lint": "eslint --fix app.js",
    "fix": "npm run format && npm run lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.0"
  },
  "license": "MIT"
}
